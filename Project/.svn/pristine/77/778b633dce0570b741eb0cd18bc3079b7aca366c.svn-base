using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace JXProduct.Component.Model
{
    public class ProductQualificationInfo
    {
        public ProductQualificationInfo()
        {
            this.StartDate = Convert.ToDateTime("1999-01-01");
            this.EndDate = Convert.ToDateTime("1999-01-01");
            this.CreateTime = DateTime.Now;
            this.ProductID = 0;
            this.ManufacturerID = 0;
            this.Status = 0;
        }

        /// <summary>
        ///自增长ID  商品资质可以自己添加需要的资质种类
        /// </summary>
        public int PQID { get; set; }

        /// <summary>
        /// 类型  Enums.
        /// </summary>
        public short Type { get; set; }
        /// <summary>
        ///资质ID
        /// </summary>
        public int QualificationID { get; set; }

        /// <summary>
        ///企业ID
        /// </summary>
        public int ManufacturerID { get; set; }

        /// <summary>
        ///商品ID
        /// </summary>
        public int ProductID { get; set; }

        /// <summary>
        ///图片路径
        /// </summary>
        public string Image { get; set; }

        /// <summary>
        ///证件编号
        /// </summary>
        public string Number { get; set; }

        /// <summary>
        ///发证时间
        /// </summary>
        public DateTime StartDate { get; set; }

        /// <summary>
        ///结束时间
        /// </summary>
        public DateTime EndDate { get; set; }

        /// <summary>
        ///保存时间
        /// </summary>
        public DateTime CreateTime { get; set; }

        /// <summary>
        ///审核状态  0 未处理  1 已处理
        /// </summary>
        public short Status { get; set; }
    }
}
