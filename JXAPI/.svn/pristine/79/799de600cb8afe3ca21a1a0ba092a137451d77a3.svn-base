using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace JXAPI.JXSdk.Domain
{
    public class AccountRecordInfo
    {
        /// <summary>
        /// 来源ID
        /// </summary>
        public long scoreID { get; set; }
        /// <summary>
        /// 用户通行证
        /// </summary>
        public int uid { get; set; }   
        /// <summary>
        /// 获得为正值，消费为负值
        /// </summary>
        public int score { get; set; }
        /// <summary>
        /// 积分余额
        /// </summary>
        public int availableScore { get; set; }
        /// <summary>
        /// 积分类型 （ 0：订单成交 1：订单追加 2：订单扣减 3：活动追加 4：微博分享 5：微博帐号绑定 6：商品回复 7：客服手工添加 8：邮箱验证获得 9：手机验证获得）
        /// </summary>
        public int scoreType { get; set; }
        /// <summary>
        /// 订单编号
        /// </summary>
        public long orderID { get; set; }
        /// <summary>
        /// 取得或消费积分的描述
        /// </summary>
        public string method { get; set; }   
        /// <summary>
        /// 时间
        /// </summary>
        public long? createTime { get; set; } 
        /// <summary>
        /// 用户名
        /// </summary>
        public string userName { get; set; }
        /// <summary>
        ///  手机号
        /// </summary>
        public string mobile { get; set; }
        /// <summary>
        /// 邮箱
        /// </summary>
        public string email { get; set; }

    }
}
